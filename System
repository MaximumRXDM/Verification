local Key = "Verified"
local Hwid = game:GetService("RbxAnalyticsService"):GetClientId()
--// players \\--
local p1 = "" -- hwid
local p2 = "4D6550CE-0088-4D2B-AA72-1F0579A4ED7C" -- hwid
local p3 = "" -- hwid

function yourscript()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/MaximumRXDM/Verification/main/MainScript'))()
end
--// keys \\--
if p1 == Hwid and Key == "Verified" then
    yourscript()
elseif
 p2 == Hwid and Key == "Verified" then
    yourscript()
elseif
 p3 == Hwid and Key == "Verified" then
    yourscript()
else
 loadstring(game:HttpGet('https://raw.githubusercontent.com/HubHexScripts/ProjectSlayers/main/Unverified'))()
 end
